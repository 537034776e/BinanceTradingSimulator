@using BinanceAPI.Models
@model List<Klines>
@{
    ViewBag.Title = "KlinesValues";
}

<h2>Valori di candela per il simbolo</h2>

@if (Model.Count() == 0)
{
    <p>non ci sono valori</p>
}
else
{
    <table>
        <tr>
            <th>Tempo di apertura</th>
            <th>Prezzo di apertura</th>
            <th>Prezzo più alto</th>
            <th>Prezzo più basso</th>
            <th>Prezzo di chiusura</th>
            <th>Volume</th>
            <th>Tempo di chiusura</th>
            <th>Volume asset</th>
            <th>Ordini</th>
            <th>Volume taker buy base</th>
            <th>Volume taker buy quote</th>
            <th>Ignore</th>
        </tr>
        @foreach (var kline in Model)
        {
            <tr>
                <td>@kline.openTime</td>
                <td>@kline.openPrice</td>
                <td>@kline.highPrice</td>
                <td>@kline.lowPrice</td>
                <td>@kline.closePrice</td>
                <td>@kline.volume</td>
                <td>@kline.kCloseTime</td>
                <td>@kline.qVolume</td>
                <td>@kline.trades</td>
                <td>@kline.tbbVolume</td>
                <td>@kline.tbqVolume</td>
                <td>@kline.ignore</td>
            </tr>
        }
    </table>
}

